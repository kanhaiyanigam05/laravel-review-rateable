{
    "name": "kanhaiyanigam05/laravel-review-rateable",
    "description": "Review & Rating system for Laravel 10, 11 & 12",
    "keywords": [
        "rating",
        "Ratable",
        "laravel",
        "Review-Rateable",
        "reviewable"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "kanhaiya"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "http://github.com/kanhaiyanigam05/laravel-review-rateable"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/config": "^12.20.0",
        "illuminate/database": "^12.20.0",
        "illuminate/support": "^12.20.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.4.0",
        "pestphp/pest": "^3.2.0"
    },
    "autoload": {
        "psr-4": {
            "kanhaiyanigam05\\ReviewRateable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kanhaiyanigam05\\ReviewRateable\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "kanhaiyanigam05\\ReviewRateable\\ReviewRateableServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
